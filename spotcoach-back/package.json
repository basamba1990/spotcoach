{
  "name": "spotcoach-back",
  "version": "1.0.0",
  "description": "Backend for Spotcoach - Advanced matching and project recommendations",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "migrate": "node dist/scripts/migrate.js",
    "worker:embedding": "node dist/workers/embedding-worker.js",
    "worker:matching": "node dist/workers/matching-worker.js",
    "worker:projects": "node dist/workers/project-suggestion-worker.js"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.0",
    "@supabase/supabase-js": "^2.38.0",
    "redis": "^4.6.0",
    "bullmq": "^4.12.0",
    "ioredis": "^5.3.0",
    "openai": "^4.28.0",
    "sweph": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.0",
    "zod": "^3.22.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.0",
    "multer": "^1.4.5",
    "sharp": "^0.32.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/node": "^20.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/multer": "^1.4.0",
    "@types/node-cron": "^3.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
