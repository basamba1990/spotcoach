{
  "version": 2,
  "builds": [
    {
      "src": "spotcoach-back/src/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "spotcoach-front/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "spotcoach-back/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "spotcoach-front/$1"
    }
  ]
}
